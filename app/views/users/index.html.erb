<div class="col-md-8 offset-md-1">
            <h2>Users</h2>
                <table class="table table-hover table-inverse">
                    <thead>
                        <tr>
                            <th>Image</th>
                            <th>Name</th>
                            <th colspan="3"></th>
                        </tr>
                    </thead>
                    <% @users.each do |book| %>
                    <tbody>
                        <tr>
                            <td><%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "no_image.jpg" %></td>
                            <td><%= @user.name %></td>
                        </tr>
                    </tbody>
                    <% end %>
                </table>
        </div>
</div>